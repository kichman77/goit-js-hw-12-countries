{{#each this}}
<div class="country-box__container">
  <h1 class="title">{{name}}</h1>
  <ul>
    <li>Capital: {{capital}}</li>
    <li>Population: {{population}}</li>
    <li>
      Languages:
      <ul>
        {{#each languages}}
        <li>{{this.name}}</li>
          {{/each}}
      </ul>
    </li>
  </ul>
  <div class="country-box__img">
    <img
      src="{{flag}}"
      width="200" alt="{{name}}" />
  </div>
</div>
{{/each}}